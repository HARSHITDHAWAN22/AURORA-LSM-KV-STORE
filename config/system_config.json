{
  "storage":{
    "memtable":{
      "max_entries": 5000
    },
    "sstable":{
      "data_directory": "data/sstables"
    }
  },

  "bloom_filter":{
    "bit_size": 10000,
    "hash_functions": 3
  },

  "compaction":{
    "strategy": "level",
    "max_files_per_level": 4,
    "interval_seconds": 5,
    "l0_threshold": 4
  },

  "flush":{
    "interval_seconds": 2
  }
}